{
    "name": "arhitov/omnireceipt-akitorg",
    "description": "AkiTorg driver for Omnireceipt fiscal receipt processing library",
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "receipt",
        "fiscal",
        "omnireceipt"
    ],
    "homepage": "https://github.com/arhitov/omnireceipt-akitorg",
    "authors": [
        {
            "name": "Alexander Arhitov",
            "email": "clgsru@gmail.com",
            "homepage": "https://github.com/arhitov/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnireceipt\\AkiTorg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnireceipt\\AkiTorg\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/uid": "*",
        "omnireceipt/common": "^1.1"
    },
    "require-dev": {
        "guzzlehttp/psr7": "*",
        "phpunit/phpunit": "*",
        "php-http/curl-client": "*",
        "php-http/mock-client": "*",
        "mockery/mockery": "*",
        "ext-readline": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
